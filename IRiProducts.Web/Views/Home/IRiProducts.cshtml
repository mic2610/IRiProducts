@model IEnumerable<IRiProducts.Business.Models.Csv.IriProduct>

@{
  ViewData["Title"] = "IRiProducts";
}

<h1>IRiProducts</h1>

<table class="table">
  <thead>
    <tr>
      <th>
        @Html.DisplayNameFor(model => model.Id)
      </th>
      <th>
        @Html.DisplayNameFor(model => model.Name)
      </th>
    </tr>
  </thead>
  <tbody>
    @foreach (var item in Model)
    {
      <tr>
        <td>
          @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
          @Html.DisplayFor(modelItem => item.Name)
        </td>
      </tr>
    }
  </tbody>
</table>
