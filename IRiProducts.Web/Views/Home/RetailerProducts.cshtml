@model IEnumerable<RetailerProduct>

@{
  ViewData["Title"] = "Retailer Products";
}

<h1>Retailer Products</h1>

<table class="table">
  <thead>
  <tr>
    <th>
      @Html.DisplayNameFor(model => model.Id)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.RetailerName)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.RetailerProductCode)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.RetailerProductCodeType)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.DateReceived)
    </th>
  </tr>
  </thead>
  <tbody>
  @foreach (var item in Model)
  {
    <tr>
      <td>
        @Html.DisplayFor(modelItem => item.Id)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.RetailerName)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.RetailerProductCode)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.RetailerProductCodeType)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.DateReceived)
      </td>
    </tr>
  }
  </tbody>
</table>